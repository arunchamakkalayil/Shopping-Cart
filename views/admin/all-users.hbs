
<section>
  <div class="container">
      <div class="row mt-4">
  <table class="table mt-6" id="all-users">
  <thead>
    <tr>
    
      <th scope="col">Name</th>
      <th scope="col">Email</th>

    </tr>
  </thead>
  <tbody>
      {{#each users}}
    <tr>
   
      <td>{{this.name}}</td>
      <td>{{this.email}}</td>
  
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</div>
</section>
<script>
  $(document).ready( function () {
    $('#all-users').DataTable();
} );
</script>
